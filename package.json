{
  "name": "boilerplate-mern",
  "version": "1.0.0",
  "description": "Boilerplate project for NodeJS based projects in TypeScript",
  "engines": {
    "npm": "6.14.13",
    "node": "14.17.3"
  },
  "scripts": {
    "build": "gulp",
    "build:e2e": "NODE_ENV=e2e gulp buildE2E",
    "e2e": "cypress run",
    "e2e:ci": "concurrently -s first -k --kill-others-on-fail \"npm run serve:e2e\" \"sleep 25 && npm run e2e\"",
    "test": "NODE_ENV=production NODE_CONFIG_ENV=testing mocha --exit -r ts-node/register --project tsconfig.json test/**/*.spec.ts",
    "lint": "eslint .",
    "lint:md": "remark .",
    "lint:fix": "eslint --fix .",
    "start": "node dist/server.js",
    "start:e2e": "node dist/src/server.js",
    "serve": "gulp serve",
    "serve:e2e": "NODE_ENV=e2e gulp serveE2E"
  },
  "author": "jalantechnologies",
  "license": "MIT",
  "dependencies": {
    "@sendgrid/mail": "^7.6.1",
    "@types/body-parser": "^1.19.2",
    "@types/mongoose": "^5.11.97",
    "bcrypt": "^5.0.1",
    "body-parser": "^1.19.1",
    "bootstrap": "^5.1.3",
    "config": "^3.3.7",
    "ejs": "^3.1.6",
    "express": "^4.17.2",
    "express-ejs-layouts": "^2.5.1",
    "google-libphonenumber": "^3.2.27",
    "js-yaml": "^4.1.0",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.2.1",
    "path": "^0.12.7",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "rollbar": "^2.24.0",
    "twilio": "^3.74.0"
  },
  "devDependencies": {
    "@babel/core": "^7.17.5",
    "@faker-js/faker": "^6.3.1",
    "@types/bcrypt": "^5.0.0",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.4",
    "@types/chai-http": "^4.2.0",
    "@types/config": "0.0.41",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.8",
    "@types/mocha": "^9.1.0",
    "@types/sinon": "^10.0.11",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "@typescript-eslint/parser": "^4.29.3",
    "babel-preset-es2015": "^6.24.1",
    "babelify": "^10.0.0",
    "browserify": "^17.0.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "chai-http": "^4.3.0",
    "concurrently": "^7.2.2",
    "cypress": "^10.3.0",
    "eslint": "^7.32.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-airbnb-typescript": "^14.0.1",
    "eslint-plugin-eslint-comments": "^3.2.0",
    "eslint-plugin-import": "^2.25.2",
    "forever": "^4.0.3",
    "gulp": "^4.0.2",
    "gulp-clean-css": "^4.3.0",
    "gulp-if": "^3.0.0",
    "gulp-minify-ejs": "^1.0.3",
    "gulp-mock-module": "^1.0.2",
    "gulp-nodemon": "^2.5.0",
